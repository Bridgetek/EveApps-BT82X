  <PropertyGroup>
    <!-- Convert $(Configuration) to uppercase -->
    <UppercaseConfiguration>$([System.String]::Copy($(Configuration)).ToUpperInvariant())</UppercaseConfiguration>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Add the DLL folder to the PATH -->
    <EveAppsDir>$(ProjectDir)..\\..\\..\\..\\</EveAppsDir>
    <EmulatorDllPath32>$(EveAppsDir)common\\eve_hal\\Bin\\Simulation\\x86</EmulatorDllPath32>
    <SPIDllPath32>$(EveAppsDir)common\\eve_hal\\Bin\MSVC\\x86</SPIDllPath32>
    <EmulatorDllPath>$(EveAppsDir)common\\eve_hal\\Bin\\Simulation\\$(Platform)</EmulatorDllPath>
    <SPIDllPath>$(EveAppsDir)common\\eve_hal\\Bin\MSVC\\$(Platform)</SPIDllPath>
    <Environment>PATH=$(EmulatorDllPath);$(SPIDllPath);$(PATH)</Environment>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <WarningLevel>Level3</WarningLevel>
      <Optimization>Disabled</Optimization>
      <!-- LCD: EVE_DISPLAY_WUXGA only -->
      <!-- EVE chip: EVE_GRAPHICS_BT820 only -->
      <!-- FT4222: EVE_PLATFORM_FT4222 -->
      <!-- MPSSE: EVE_PLATFORM_MPSSE -->
      <!-- EMU: EVE_PLATFORM_BT8XXEMU - not yet supported in BT820-->
      <PreprocessorDefinitions>$(UppercaseConfiguration);$(Platform);%(PreprocessorDefinitions);EVE_GRAPHICS_BT820;EVE_PLATFORM_MPSSE;EVE_DISPLAY_WUXGA;</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>$(EveAppsDir)Common\\application;$(EveAppsDir)Common\\eve_hal;$(EveAppsDir)Common\\eve_hal\\Hdr;..\..\Hdr\;..\..\Src;</AdditionalIncludeDirectories>
      <AdditionalOptions>/VERBOSE /utf-8 </AdditionalOptions>
    </ClCompile>
    <Link>
      <SubSystem>Console</SubSystem>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <AdditionalLibraryDirectories>$(EmulatorDllPath);$(SPIDllPath);$(OutDir)</AdditionalLibraryDirectories>
      <AdditionalDependencies>kernel32.lib;user32.lib;gdi32.lib;winspool.lib;comdlg32.lib;advapi32.lib;shell32.lib;ole32.lib;oleaut32.lib;uuid.lib;odbc32.lib;odbccp32.lib;%(AdditionalDependencies);libMPSSE.lib;ftd2xx.lib;LibFT4222.lib</AdditionalDependencies>
    </Link>
    <PreBuildEvent>
      <Command>
        mkdir "$(OutDir)"
        echo xcopy /ecfhrsy  "$(EmulatorDllPath32)\\*.*" "$(OutDir)"
        echo xcopy /ecfhrsy  "$(SPIDllPath32)\\*.*" "$(OutDir)"
        echo xcopy /ecfhrsy  "$(EmulatorDllPath)\\*.*" "$(OutDir)"
        echo xcopy /ecfhrsy  "$(SPIDllPath)\\*.*" "$(OutDir)"
        echo xcopy /ecfhrsy  "$(ProjectDir)..\..\Test\Flash\*.bin" "$(OutDir)\__Flash.bin"
        echo D | xcopy /ecfhrsy  "$(EmulatorDllPath32)\\*.*" "$(OutDir)"
        echo D | xcopy /ecfhrsy  "$(SPIDllPath32)\\*.*" "$(OutDir)"
        echo D | xcopy /ecfhrsy  "$(EmulatorDllPath)\\*.*" "$(OutDir)"
        echo D | xcopy /ecfhrsy  "$(SPIDllPath)\\*.*" "$(OutDir)"
        echo F | xcopy /ecfhrsy  "$(ProjectDir)..\..\Test\Flash\*.bin" "$(OutDir)\__Flash.bin"
        exit 0
      </Command>
    </PreBuildEvent>
